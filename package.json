  "scripts": {
    "build:client": "webpack -d && webpack-dev-server --content-base client --inline --hot",
    "build": "babel --presets es2015 server -d lib",
    "start": "npm run build && node lib/bin/www.js",
    "server": "nodemon --watch server --exec babel-node -- server/bin/www.js",
    "start:dev": "babel-node server/bin/www.js",
    "test:report": "nyc --reporter=html --reporter=text mocha --timeout 5000 --compilers js:babel-core/register server/src/test/test.js",
    "test:dev": "mocha --timeout 500000 --compilers js:babel-core/register server/src/test/test.js",
    "test": "NODE_ENV=test npm run migration && NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 5000 --compilers js:babel-core/register server/src/test/test.js",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage:dev": "nyc npm test  && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migration": "sequelize db:migrate",
    "postinstall": "npm run migration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noblemajesty/more-recipe.git"
